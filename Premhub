   if v.Name == "ReloadTime" then
      v.Value = 0.000000000.5
   end
   if v.Name == "FireRate" then
       v.Value = 0.000000000000000.5
   end
   if v.Name == "Crit" then
       v.Value = 999999
   end
end
	end)
	end
